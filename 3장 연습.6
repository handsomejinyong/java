package gongbu;
import java.util.*;
public class llsa {

	public static void main(String[] args) {
		int[][] gugudan=new int[10][10];
		System.out.println("*** 구구단 ***");
		for(int i=1; i<=9;i++)
		{
			for(int j=1;j<=9;j++)
			{
				gugudan[i][j]=i*j;
			}
		}
		for(int i=1;i<=9;i++)
		{
			for(int j=1;j<=9;j++)
			{
				System.out.print(i+"*"+j+"="+gugudan[i][j]+" ");
			}
			System.out.println();
		}
		
	}
		
	
	
	
}


