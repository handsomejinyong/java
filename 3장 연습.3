package javasealsup;
import java.util.*;

public class car {

	
	
	public static void main(String args[]) {
		int a,b;
		Scanner num=new Scanner(System.in);
		
		
	
		while(true)
		{
			System.out.print("첫번째 정수를 입력하시오 : ");
			a=num.nextInt();
			
			System.out.print("두번째 정수를 입력하시오 : ");
			b=num.nextInt();
			if(a*b>500)
			{
			System.out.println("a*b=: "+a*b);
			System.out.print("두 수의 곱이 500을 초과하였습니다.");
			break;
			}
			else
			{
				System.out.println("a*b=: "+a*b);

			}
		}
	}

}
