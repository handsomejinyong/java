package 연습3;

public class 연습3 {

	public static void main(String[] args) {
		int x=300;
		int y=300;
		int xnanum=x/42;	//나누기 연산
		int xnamu=x%42;		//나머지 연산
		
		int ynanum=y/38; 	//나누기 연산
		int ynamu=y%38;		//나머지 연산
		
		System.out.println("x="+x);
		System.out.println("y="+y);
		System.out.println("x를 42로 나눈 몫은"+xnanum+","+"나머지는"+xnamu+"입니다.");
		System.out.println("y를 38로 나눈 몫은"+ynanum+","+"나머지는"+ynamu+"입니다.");
	}

}
