package 연습2;
import java.util.*;
public class 연습2 {

	public static void main(String[] args) {
		Scanner put= new Scanner(System.in);
		int a;
		int b;
		//변수 선언
		
		System.out.print("두개의 정수를 입력하시오: ");
		a=put.nextInt();
		b=put.nextInt();
		//a와 b에 값 입력
		int plus=a+b;
		int min=a-b;
		int gop=a*b;
		int nanu=a/b;
		int namu=a%b;
		//각 연산값 저장
		System.out.println("a+b="+plus);
		System.out.println("a-b="+min);
		System.out.println("a*b="+gop);
		System.out.println("a/b="+nanu);
		System.out.println("a%b="+namu);
		//각 결과값 출력
		
		
		

	}

}
