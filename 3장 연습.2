package javasealsup;
import java.util.*;

public class car {

	
	
	public static void main(String args[]) {
		int a,b;
		Scanner num=new Scanner(System.in);
		System.out.print("첫번째 정수를 입력하시오 : ");
		a=num.nextInt();
		System.out.print("두번째 정수를 입력하시오 : ");

		b=num.nextInt();
	
		if(a>b)
		{
			System.out.print("큰값 : "+a+"\t"+"작은 값 : "+b);
		}
		else
		{
			System.out.print("큰값 : "+b+"\n"+"작은 값 : "+a);
		}
		
	}

}
